{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts",
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts",
    "src/environments/environment.prod.ts",
    "src/polyfills.ts",
    "src/app/app.config.server.ts",
    "src/main.server.ts",
    "server.ts"
  ]
}
